# Environment Variables Template
# Copy this file to .env and fill in your values

# Supabase Configuration
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_PUBLISHABLE_KEY=your-supabase-anon-key
VITE_SUPABASE_PROJECT_ID=local

# Admin Panel
VITE_ADMIN_PASSWORD=change-this-to-a-strong-password

# Database Configuration
POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=change-this-to-a-strong-password

# JWT Secret (minimum 32 characters)
JWT_SECRET=change-this-to-a-cryptographically-secure-secret-at-least-32-chars

# Supabase Service Key
SUPABASE_SERVICE_KEY=your-supabase-service-role-key

# Cloudflare Tunnel Token (for docker-compose)
CLOUDFLARE_TUNNEL_TOKEN=your-tunnel-token-here
